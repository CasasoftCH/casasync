{
    "name": "casasoft/casawp",
    "description": "casawp WordPress Plugin",
    "license": "",
    "keywords": [
        "casawp",
        "casasoft"
    ],
    "homepage": "http://framework.zend.com/",
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:CasasoftCH/casamodules.git"
        }
    ],
    "require": {
        "php": "^7.4 || ~8.0.0 || ~8.1.0 || ~8.2.0 || ~8.3.0",
        "laminas/laminas-loader": "^2.0",
        "laminas/laminas-mvc": "^3.7",
        "laminas/laminas-i18n": "^2.0",
        "laminas/laminas-modulemanager": "^2.16",
        "laminas/laminas-view": "^2.0",
        "laminas/laminas-serializer": "^2.0",
        "laminas/laminas-log": "^2.0",
        "laminas/laminas-servicemanager": "^3.23",
        "laminas/laminas-escaper": "^2.0",
        "casasoft/casamodules": "dev-casawp",
        "laminas/laminas-mvc-i18n": "^1.9",
        "laminas/laminas-form": "^3.21"
    },
    "autoload": {
        "psr-4": {
            "casawp\\": "classes/",
            "CasasoftStandards\\": "vendor/casasoft/casamodules/CasasoftStandards/src/",
            "CasasoftMessenger\\": "vendor/casasoft/casamodules/CasasoftMessenger/src/"
        }
    },
    "config": {
        "allow-plugins": {
            "laminas/laminas-dependency-plugin": true
        }
    }
}
